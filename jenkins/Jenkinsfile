pipeline {
    agent none

    stages {
        stage('UI Tests') {
            agent {
                docker {
                    filename 'Dockerfile'   // tai joku muu nimi jos eri
                    additionalBuildArgs '--pull'
                }
            }
            steps {
                dir('tests') {
                    sh 'robot --version'
                    sh 'ls'
                    sh 'robot -d results .'

                }
            }

        }
    }
}